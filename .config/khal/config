[calendars]

  [[ient]]
    path = ~/calendars/ient/calendar
    color = dark blue

  [[institut]]
    path = ~/calendars/institut/calendar
    color = dark green
    readonly = True

  [[ncpersonal]]
    path = ~/calendars/nextcloud/personal
    color = yellow
    
[default]
timedelta = 5d
